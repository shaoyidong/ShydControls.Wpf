            <GradientStop Offset="0" Color="{DynamicResource Color5}" />
        </RadialGradientBrush>
        <Color x:Key="ColorOn">
            #c0ffc0
        </Color>
        <Color x:Key="ColorOff">
            #ffc0c0
        </Color>
        <!--<SolidColorBrush
            x:Key="BrushMainShutter"
            x:Name="BrushMainShutter"
            Color="{StaticResource ColorOff}" />-->
