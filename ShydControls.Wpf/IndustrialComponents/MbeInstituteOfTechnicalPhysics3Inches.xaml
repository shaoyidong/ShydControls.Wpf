            <GradientStop Offset="0" Color="{DynamicResource Color1}" />
            <GradientStop Offset="0.45" Color="{DynamicResource Color2}" />
            <GradientStop Offset="0.77" Color="{DynamicResource Color3}" />
            <GradientStop Offset="1" Color="{DynamicResource Color4}" />
        </LinearGradientBrush>
        <LinearGradientBrush x:Key="BrushH" StartPoint="0,0.5" EndPoint="1,0.5">
