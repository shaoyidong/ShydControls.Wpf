        <!--<Storyboard x:Key="gv11OffSb" Timeline.DesiredFrameRate="30" />
        <Storyboard x:Key="gv11OnSb" Timeline.DesiredFrameRate="30" />-->
        <Storyboard
            x:Key="rotationStoryboard"
            RepeatBehavior="Forever"
            Timeline.DesiredFrameRate="30"
            Duration="0:0:1">
            <DoubleAnimation
                Storyboard.TargetName="manipulatorRt"
                Storyboard.TargetProperty="RenderTransform.Children[0].Angle"
                From="0"
                To="360"
                Duration="0:0:1" />
        </Storyboard>
        <Storyboard
            x:Key="mainShutterOnStoryboard"
            Timeline.DesiredFrameRate="30"
            Duration="0:0:1.5">
            <DoubleAnimation
                Storyboard.TargetName="canvasMainShutter"
                Storyboard.TargetProperty="RenderTransform.Children[1].Angle"
                To="46"
                Duration="0:0:1.5">
                <DoubleAnimation.EasingFunction>
                    <SineEase EasingMode="EaseOut" />
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <ColorAnimation
                Storyboard.TargetName="mainShutterBg"
                Storyboard.TargetProperty="Color"
                To="{StaticResource ColorOn}"
                Duration="0:0:1.5" />
        </Storyboard>
        <Storyboard
            x:Key="mainShutterOffStoryboard"
            Timeline.DesiredFrameRate="30"
            Duration="0:0:1.5">
            <DoubleAnimation
                Storyboard.TargetName="canvasMainShutter"
                Storyboard.TargetProperty="RenderTransform.Children[1].Angle"
                To="0"
                Duration="0:0:1.5">
                <DoubleAnimation.EasingFunction>
                    <SineEase EasingMode="EaseOut" />
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <ColorAnimation
                Storyboard.TargetName="mainShutterBg"
                Storyboard.TargetProperty="Color"
                To="{StaticResource ColorOff}"
                Duration="0:0:1.5" />
        </Storyboard>
    </UserControl.Resources>

