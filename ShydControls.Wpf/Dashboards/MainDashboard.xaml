<UserControl
    x:Class="ShydControls.Wpf.Dashboards.MainDashboard"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ShydControls.Wpf.Dashboards"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="200"
    d:DesignWidth="200"
    mc:Ignorable="d">
    <Grid>
        <Image Source="pack://application:,,,/ShydControls.Wpf;component/Assets/Images/542654.png" Stretch="Uniform" />
        <Image
            x:Name="arcImage"
            Source="pack://application:,,,/ShydControls.Wpf;component/Assets/Images/542654265.png"
            Stretch="Uniform" />
        <Image
            RenderTransformOrigin="0.5 0.5"
            Source="pack://application:,,,/ShydControls.Wpf;component/Assets/Images/5625426.png"
            Stretch="Uniform">
            <Image.RenderTransform>
                <TransformGroup>
                    <RotateTransform Angle="-272" />
                    <RotateTransform x:Name="rot" />
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
    </Grid>
</UserControl>
